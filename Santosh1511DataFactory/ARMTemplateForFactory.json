		{
			"name": "[concat(parameters('factoryName'), '/santoshVM')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "191.237.252.118,1433",
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('santoshVM_connectionString')]"
				}
			},
			"dependsOn": []
